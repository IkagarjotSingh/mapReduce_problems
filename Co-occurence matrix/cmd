$HADOOP_HOME/bin/hadoop jar $HADOOP_HOME/share/hadoop/tools/lib/hadoop-streaming-2.7.6.jar -D stream.num.map.output.key.fields=2 -D mapred.text.key.partitioner.options=-k1,1 -D mapred.reduce.tasks=10 -input /user/retail.dat -output /output/ -file mapperRetail.py -file reducerRetail.py -mapper mapperRetail.py -reducer reducerRetail.py -partitioner org.apache.hadoop.mapred.lib.KeyFieldBasedPartitioner