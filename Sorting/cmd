$HADOOP_HOME/bin/hadoop jar $HADOOP_HOME/share/hadoop/tools/lib/hadoop-streaming-2.7.6.jar -D mapred.reduce.tasks=4 -D stream.num.map.output.key.fields=2 -D mapred.text.key.partitioner.options=-k1,1 -input /user/shakespeare.txt -output /output/ -file mapperSort.py -file reducerSort.py -mapper mapperSort.py -reducer reducerSort.py -partitioner org.apache.hadoop.mapred.lib.KeyFieldBasedPartitioner